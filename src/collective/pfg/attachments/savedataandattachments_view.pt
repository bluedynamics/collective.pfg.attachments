<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="bda.htu.protocol">

<body>

  <metal:content-core fill-slot="content-core">
    <metal:block define-macro="content-core">

      <table tal:define="rows view/rows">
        <tr tal:repeat="row rows">
          <tal:td repeat="col row">
            <span tal:condition="python: col['type'] == 'string'"
                  tal:replace="col/value">value</span>
            <a href=""
               tal:condition="python: col['type'] == 'attachment'"
               tal:attributes="href col/url"
               tal:content="col/value">value</a>
          </tal:td>
        </tr>
      </table>

    </metal:block>
  </metal:content-core>

</body>

</html>
